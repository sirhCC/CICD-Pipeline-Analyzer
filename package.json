{
  "name": "@sirhcc/cicd-pipeline-analyzer",
  "version": "1.0.0",
  "description": "Enterprise-grade CI/CD Pipeline Analyzer with intelligent insights, multi-provider support, and real-time monitoring",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "cicd",
    "pipeline",
    "analyzer",
    "github-actions",
    "gitlab-ci",
    "jenkins",
    "devops",
    "monitoring",
    "performance",
    "enterprise"
  ],
  "author": "sirhCC",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "setup": "npm run clean && npm run build",
    "docker:build": "docker build -t cicd-pipeline-analyzer .",
    "docker:run": "docker run -p 3000:3000 cicd-pipeline-analyzer"
  },
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "bull": "^4.11.5",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.2.2",
    "tsx": "^4.1.0",
    "tsc-alias": "^1.8.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "nodemon": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sirhCC/cicd-pipeline-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/sirhCC/cicd-pipeline-analyzer/issues"
  },
  "homepage": "https://github.com/sirhCC/cicd-pipeline-analyzer#readme"
}
